  // ======= About Section =======
  section#compare
    .container.px-10
      .section-title.p-0(data-aos='fade-in' data-aos-delay='10')
        br
        h2(style="font-size:50px") Compare PPPs
        div(style="display:flex; justify-content: center; margin-top: 40px")
          div(style="display:flex; font-size: 30px; padding-right: 70px")
            a(href='/inner-page' style="font-weight: bold; color: #196be6") Go to Our Map
            span(class="material-icons" style="font-weight: bold; color: #196be6; display: block; margin-top: auto; margin-bottom: auto") arrow_forward_ios
          div(style="display:flex; font-size: 30px;")
            a(href='/inner-page' style="font-weight: bold; color:#196be6") Go to Dashboard
            span(class="material-icons" style="font-weight: bold; color: #196be6; display: block; margin-top: auto; margin-bottom: auto") arrow_forward_ios

      .panel(data-aos='fade-in' data-aos-delay='80')
            p
            table(style="width:70%; margin:auto;")
              tbody
                tr 
                  td(style="border-top: 2px solid black; border-bottom: 2px solid black; border-right: 2px solid black; width: 20%; background-color: #ececdf")
                      strong Project Name
                  td(style="border-bottom: 2px solid black; border-top: 2px solid black; display:flex")
                      input(style="border: 1px solid black; width:220px; margin-right: 10px" placeholder=" Enter the project name")
                      span(class="material-icons" style="display: block; margin-top: auto; margin-bottom: auto; cursor: pointer") search
                tr
                  td(style="border-bottom: 2px solid black; border-right: 2px solid black; width: 20%; background-color: #ececdf")
                      strong Income Group
                  td(style="border-bottom: 2px solid black")
                      //- 확대했을 때 테이블 깨지는 문제 발생
                      select(style="border: 1px solid black; width:280px")
                        option(style="display: none")
                        option(value="High Income") High Icome
                        option(value="Low Income") Low Income
                        option(value="Lower middle income") Lower middle income
                        option(value="Upper middle income") Upper middle income

                tr
                  td(style="border-bottom: 2px solid black; border-right: 2px solid black; width: 20%; background-color: #ececdf")
                      strong Status
                  td(style="border-bottom: 2px solid black;")
                      select(style="border: 1px solid black; width:280px")
                        option(style="display: none")
                        option(value="High Income") Status1
                        option(value="Low Income") Status2
                        option(value="Lower middle income") Status3
                        option(value="Upper middle income") Status4
                tr
                  td(style="border-bottom: 2px solid black; border-right: 2px solid black; width: 20%; background-color: #ececdf")
                      strong Primary Sector
                  td(style="border-bottom: 2px solid black;")
                      select(class="primeSector" style="border: 1px solid black; width:280px; color: black; font-weight: 600")
                        option(value="Energy") Energy
                        option(value="ICT") ICT
                        option(value="Transport") Transport
                        option(value="Water and Sewerage") Water and Sewerage
                        option(value="Solid Waste") Solid Waste
                        option(value="Other Sector") Other Sector
                tr
                  td(style="border-bottom: 2px solid black; border-right: 2px solid black; width: 20%; background-color: #ececdf")
                      strong Sub Sector
                  td(style="border-bottom: 2px solid black;")
                      //- 앞 Primary Sector에 따라 다른 값 가져야 하는 문제
                      //- PUG 문법에서 이런 기능이 가능한지가... JS에서 다뤄야 할 거 같은데
                      select(style="border: 1px solid black; width:180px; color: black; font-weight: 600")
                        option(value="Electricity") Electricity
                        option(value="Coal/Mine") Coal/Mine
                        option(value="Natural Gas Pipeline") Natural Gas Pipeline
                        option(value="Oil Pipeline") Oil Pipeline
              br
      .section-title.p-0(data-aos='fade-in' data-aos-delay='100')
