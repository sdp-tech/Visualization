main#main
  // ======= Breadcrumbs Section =======
  section.breadcrumbs
    .container
      .d-flex.justify-content-between.align-items-center
        h2 Map
        ol
          li
            a(href='index') Home
          li Map
  // End Breadcrumbs Section
  section.inner-page
    // tutorial
    #tutorial.modal
      #tutorial_text.modal-content
        // modal header
        .modal-header
          p(style='float: left;font-size: 24px; margin:0px; font-weight: bold;') Tutorial
          span#tutorial_close.close(onclick='popup_close();') &times;
        // modal body
        .modal-body
          #0.tutorial_content
            img.tutorial_img(src='assets/img/tutorial_image/tutorial1.png' alt='alternatetext')
          #1.tutorial_content
            img.tutorial_img(src='assets/img/tutorial_image/tutorial2.png' alt='alternatetext')
          #2.tutorial_content
            img.tutorial_img(src='assets/img/tutorial_image/tutorial3.png' alt='alternatetext')
        br
        p.button_control(style='text-align: center;')
          button#previous.innerbtn_tutorial(onclick='tutorialpage(previous)') &lt;
          button#next.innerbtn_tutorial(onclick='tutorialpage(next)') &gt;
    // tutorial
    .container
      br
      p(style='text-align: right;')
        button#tutorial_btn(onclick='tutorial_on()')
          span Tutorial
      // MAP GOES HERE
      // sidemenu
      #mapwrap.mapcontainer.wrapper
        #toolbar
          a.hamburger(onclick='toolbar_open()' style='cursor: pointer;')
            span Filters
          // filters
          #filters
            h4
              a.tooltips(href='#' data-toggle='tooltip' title='filterbar') Filter bar
            .filter_list
            ul
              // // Region
              li.listclass_filter
                h6
                  a(href='#' data-toggle='tooltip' title='Click the title of the region to select/unselect all of the corresponding countries.') Region
                  button#clearEach.region_clear
                    span clear
                select#country-select.select.js-select2-multi.country-select(name='countries[]' multiple='multiple' style='width: 90%')
              // // Sector
              li.listclass_filter
                h6
                  a(href='#' data-toggle='tooltip' title='Click the title of the sector to select/unselect all of the corresponding subsectors.') Sector
                  button#clearEach.sector_clear
                    span clear
                select#sector-select.select.js-select2-multi.sector-select(name='sector[]' multiple='multiple' style='width: 90%')
              // // FC Year
              li.listclass_filter
                h6
                  a(href='#' data-toggle='tooltip' title='Description goes here') FC Year
                  button#clearEach.fcyear_clear
                    span clear
                input#myCheck(type='checkbox' onclick='includeNA()' checked='true')
                |  Include N/A value 
                input.js-range-slider(type='text' id='#demo_2' name='my_range' value='')
              // // Status
              li.listclass_filter
                h6
                  a(href='#' data-toggle='tooltip' title='Description goes here') Status
                  button#clearEach.status_clear
                    span clear
                select#status-select.select.js-select2-multi.status-select(name='status[]' multiple='multiple' style='width: 90%')
              // // Income Group
              li.listclass_filter
                h6
                  a(href='#' data-toggle='tooltip' title='Description goes here') Income Group
                  button#clearEach.income_clear
                    span clear
                select#income-select.select.js-select2-multi.income-select(name='income[]' multiple='multiple' style='width: 90%')
              // // PPI Type
              li.listclass_filter
                h6
                  a(href='#' data-toggle='tooltip' title='Description goes here') Type of PPI
                  button#clearEach.ppitype_clear
                    span clear
                select#ppitype-select.select.js-select2-multi.ppitype-select(name='ppitype[]' multiple='multiple' style='width: 90%')
            // button
            button.button.clearfilter(style='vertical-align:middle')
              span
                a.tooltips(href='#' data-toggle='tooltip' title='Clear All') Clear Filters
      p
